<p id="notice"><%= notice %></p>

<h1>Listing Holdings</h1>

<table>
  <thead>
    <tr>
      <th>Holding</th>
      <th>Leve</th>
      <th>Bastionlevel</th>
      <th>Provinceprotects</th>
      <th>Visibility</th>
      <th>Contested</th>
      <th>Name</th>
      <th>Type</th>
      <th>Originalname</th>
      <th>Ruledupthisturn</th>
      <th>Rpexpenditurethisturn</th>
      <th>Gpexpenditurethisturn</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @holdings.each do |holding| %>
      <tr>
        <td><%= holding.holding_id %></td>
        <td><%= holding.leve %></td>
        <td><%= holding.bastionLevel %></td>
        <td><%= holding.provinceProtects %></td>
        <td><%= holding.visibility %></td>
        <td><%= holding.contested %></td>
        <td><%= holding.name %></td>
        <td><%= holding.type %></td>
        <td><%= holding.originalName %></td>
        <td><%= holding.ruledUpThisTurn %></td>
        <td><%= holding.rpExpenditureThisTurn %></td>
        <td><%= holding.gpExpenditureTHisTUrn %></td>
        <td><%= link_to 'Show', holding %></td>
        <td><%= link_to 'Edit', edit_holding_path(holding) %></td>
        <td><%= link_to 'Destroy', holding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

